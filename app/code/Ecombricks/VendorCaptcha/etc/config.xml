<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <customer>
            <captcha>
                <shown_to_logged_in_user>
                    <contact_vendor>1</contact_vendor>
                </shown_to_logged_in_user>
                <always_for>
                    <contact_vendor>1</contact_vendor>
                </always_for>
            </captcha>
        </customer>
        <captcha>
            <frontend>
                <areas>
                    <contact_vendor>
                        <label>Contact Vendor</label>
                    </contact_vendor>
                </areas>
            </frontend>
        </captcha>
    </default>
</config>