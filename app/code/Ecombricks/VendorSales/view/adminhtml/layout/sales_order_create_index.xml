<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceBlock name="order_content">
                <referenceBlock name="data">
                    <referenceBlock name="sidebar">
                        <referenceBlock name="wishlist">
                            <action method="setTemplate">
                                <argument name="template" xsi:type="string">Ecombricks_VendorSales::order/create/sidebar/items.phtml</argument>
                            </action>
                        </referenceBlock>
                    </referenceBlock>
                    <action method="setTemplate">
                        <argument name="template" xsi:type="string">Ecombricks_VendorSales::order/create/data.phtml</argument>
                    </action>
                </referenceBlock>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>